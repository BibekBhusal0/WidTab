{
  "manifest_version": 3,
  "name": "Productivity App",
  "description": "Productivity App for Chrome, Manage Todo-list, Habit tracker and Bookmark .",
  "options_page": "src/pages/options/index.html",
  "action": {
    "default_popup": "src/pages/popup/index.html",
    "default_icon": {
      "32": "icon-32.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "src/pages/newtab/index.html"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "optional_permissions": [
    "topSites"
  ],
  "version": "0.0.0",
  "permissions": [
    "bookmarks",
    "storage",
    "unlimitedStorage",
    "favicon"
  ]
}
